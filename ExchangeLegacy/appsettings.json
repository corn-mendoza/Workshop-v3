{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Error",
      "System": "Error",
      "Microsoft": "Error",
      "A": "Error",
      "Steeltoe": "Error" 
    }
  },
  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication",
      "cloudfoundry": {
        "validateCertificates": false
      }
    }
  },
  "spring": {
    "application": {
      "name": "Exchange_BTCUSD"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": true,
      "validate_certificates": false
    },
    "instance": {
      "hostName": "localhost",
      "port": 58599
    }
  },
  "mysql": {
    "client": {
      "sslmode": "none",
      "server": "localhost",
      "username": "root",
      "database": "exchange2"
    }
  },
  "hystrix": {
    "stream": {
      "validate_certificates": false  
    }
  }
}
